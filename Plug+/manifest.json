{
  "name": "Plug+",
  "version": "1.0.2.7",
  "manifest_version": 2,
  "description": "A plug.dj script chrome plugin.",
  "icons": {
	  "64": "icon.png",
	  "267": "largeIcon.png"
  },
  "browser_action": {
	"default_title": "Plug+",
    "default_icon": "icon.png",
	"default_popup": "menu.html"
  },
  "permissions": [
    "*://*.plug.dj"
  ]
}
